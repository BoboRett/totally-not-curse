{
    "name": "totallynotcurse",
    "version": "1.0.0",
    "main": "main/main.js",
    "author": "BoboRett <BoboRett@users.noreply.github.com>",
    "license": "MIT",
    "build": {
        "appId": "com.electron.tnc",
        "files": [
            "build/**/*",
            "main/**/*"
        ],
        "mac": {
            "category": "addon-manager"
        },
        "productName": "Totally Not Curse"
    },
    "scripts": {
        "dev:js": "webpack-dev-server",
        "dev:electron": "electron .",
        "dev": "concurrently -k -n js,electron -c green,cyan \"yarn run dev:js\" \"yarn run dev:electron\"",
        "pack": "webpack -p && electron-builder --dir",
        "dist": "webpack -p && electron-builder"
    },
    "dependencies": {
        "electron-is-dev": "^1.2.0",
        "electron-reload": "^1.5.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1"
    },
    "devDependencies": {
        "@babel/core": "^7.11.5",
        "@babel/preset-env": "^7.11.5",
        "@babel/preset-react": "^7.10.4",
        "babel-loader": "^8.1.0",
        "concurrently": "^5.3.0",
        "css-loader": "^4.2.2",
        "electron": "^10.1.1",
        "electron-builder": "^22.8.0",
        "eslint": "^7.8.1",
        "eslint-loader": "^4.0.2",
        "eslint-plugin-react": "^7.20.6",
        "file-loader": "^6.1.0",
        "html-webpack-plugin": "^4.4.1",
        "less": "^3.12.2",
        "less-loader": "^7.0.0",
        "mini-css-extract-plugin": "^0.11.0",
        "webpack": "^4.44.1",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.0"
    },
    "babel": {
        "presets": [
            "@babel/preset-env",
            "@babel/preset-react"
        ]
    }
}
